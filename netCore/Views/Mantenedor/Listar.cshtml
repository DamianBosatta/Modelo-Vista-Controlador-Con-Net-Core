@model List<UnidadesModel>
@{
    ViewData["Title"] = "Listar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="card">
    <div class="card-header">
        Lista Automoviles
    </div>
    <div class="card-body">
<a asp-action="Crear"asp-controller="Mantenedor" class=" btn btn.success"> Cargar Automovil </a>
<hr />
        <table class =" table table-bordered">
            <thead>
                <tr>

                    <th> Modelo</th>
                    <th> Marca</th>
                    <th> Año</th>
                    <th> Kilometros</th>
                    <th> Precio</th>
                    <th>        </th>






                </tr>





            </thead>

            <tbody>
                
                @foreach(var variable in Model)
            {
                <tr>
                    <td>@variable.modelo </td>
                    <td>@variable.marca </td>
                    <td>@variable.año </td>
                    <td>@variable.kilometros </td>
                    <td>@variable.precio </td>
                    <td>           <a asp-action="Editar" asp-controller="Mantenedor" class=" btn btn.primary btn-sm"> Actualizar Automovil </a>        </td>

                </tr>


            }


            </tbody>







        </table>
    </div>

</div>
